#include <stdio.h>
int main()
{
    int year;
    printf("ENTER A YEAR: ");
    scanf("%d",&year);
    if(year%4==0)
    {
        printf("IT\'S A LEAP YEAR");
    }
    else if(year%100==0)
    {
        printf("IT\'S A LEAP YEAR");
    }
    else if(year%400==0)
    {
        printf("IT\'S A LEAP YEAR");
    }
    else
    {
        printf("IT\'S NOT A LEAP YEAR");
    }
    return 0;
}
