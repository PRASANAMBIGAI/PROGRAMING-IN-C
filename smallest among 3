#include <stdio.h>
int main()
{
    int p,q,r;
    printf("ENTER THREE INTEGERS: ");
    scanf("%d%d%d",&p,&q,&r);
    if(p<q && p<r)
    {
        printf("THE FIRST ENTERED INTEGER IS THE SMALLEST");
    }
    else if(q<p && q<r)
    {
        printf("THE SECOND ENTERED INTEGER IS THE SMALLEST."); 
    }
    else 
    {
        printf("THE THIRD ENTERED INTEGER IS THE SMALLEST."); 
    }
    return 0;
}
